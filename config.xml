<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vaenow.prddemo" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PRDDemo</name>
    <description>
         A demo for Cordova plugin: cordova-plugin-pause-resume-download. 断点续下载，DEMO 
    </description>
    <author email="luowenbiz@foxmail.com" href="http://cordova.io">
        LuoWen
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-pause-resume-download" spec="~1.0.1" />
</widget>
