<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vaenow.prddemo" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>PRDDemo</name>
    <description>
         A demo for Cordova plugin: cordova-plugin-pause-resume-download. 断点续下载，DEMO 
    </description>
    <author email="luowenbiz@foxmail.com" href="http://cordova.io">
        LuoWen
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
